# Server Configuration
KADMIN_SERVER_PORT=:8080
KADMIN_SERVER_MODE=debug

# Database Configuration
KADMIN_DATABASE_HOST=localhost
KADMIN_DATABASE_PORT=3306
KADMIN_DATABASE_NAME=k_admin
KADMIN_DATABASE_USERNAME=root
KADMIN_DATABASE_PASSWORD=password
KADMIN_DATABASE_MAX_IDLE_CONNS=10
KADMIN_DATABASE_MAX_OPEN_CONNS=100

# JWT Configuration
KADMIN_JWT_SECRET=your-secret-key-change-this-in-production
KADMIN_JWT_ACCESS_EXPIRATION=15
KADMIN_JWT_REFRESH_EXPIRATION=7

# Redis Configuration
KADMIN_REDIS_HOST=localhost
KADMIN_REDIS_PORT=6379
KADMIN_REDIS_PASSWORD=
KADMIN_REDIS_DB=0

# Logger Configuration
KADMIN_LOGGER_LEVEL=info
KADMIN_LOGGER_PATH=./logs/app.log
KADMIN_LOGGER_MAX_SIZE=100
KADMIN_LOGGER_MAX_AGE=7
KADMIN_LOGGER_MAX_BACKUPS=3
KADMIN_LOGGER_COMPRESS=true
